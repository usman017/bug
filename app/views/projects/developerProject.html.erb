

<html lang="en">
<head>
  <title> All project</title>

</head>

<body>
<h2>All projects</h2>

<% flash.each do |type, msg| %>
  <div>
    <div class="alert alert-primary" role="alert">
      <%=msg %>
    </div>
  </div>
<% end %>
<div class="container"  id="container">
    <table class="table">
      <tr>
        <th> Project Name </th>
        <th> All bug</th>
      </tr>
      <% @projects.each do |project| %>
        <tr>



       <td> <%=project.project.name %></td>
          <td> <%=link_to 'Bugs ',showBug_path({project_id:project.project.id})%> </td>
        </tr>
      <% end %>
    </table>
</div>
</body>
</html>